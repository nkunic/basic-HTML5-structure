// BUTTONS


@mixin add-to-wishlist-button-hollow () {
padding: 0;
color:$brand-primary-color;
background-color: transparent;
transition: all 0.3s ease 0s;
-webkit-transition: all 0.3s ease 0s;
-moz-transition: all 0.3s ease 0s;
	&:hover {
	color:$brand-secondary-color;	
	}
	&:hover, &:active, &:focus {
    /*border: none;*/
    outline: none;
    box-shadow: none;
    }    
}


@mixin add-to-wishlist-button-full () {
padding: 0;
color: $brand-secondary-color;
background-color: transparent;
transition: all 0.3s ease 0s;
-webkit-transition: all 0.3s ease 0s;
-moz-transition: all 0.3s ease 0s;
    &:hover {
    color:$brand-primary-color;   
    }    
    &:hover, &:active, &:focus {
    /*border: none;*/
    outline: none;
    box-shadow: none;
    }    
}


@mixin add-to-wishlist-hollow-heart () {
padding: 5px 7px;  
color: $brand-secondary-color;
font-family: $body-font;
font-size: 13px;
line-height: 1.5;
font-weight: 500;
vertical-align: middle;
text-transform: uppercase;
background-color: $white;
border-radius: 0;
border: 1px solid $brand-primary-color;
min-width: 156px;
transition: all 0.3s ease 0s;
-webkit-transition: all 0.3s ease 0s;
-moz-transition: all 0.3s ease 0s;
    i {
    color: $brand-primary-color;
    font-size: 20px;
    line-height: 1;
    vertical-align: middle;
    margin-right: 5px;    
    }
    &:hover {
    color: $brand-secondary-color;
    background-color: $white;
    border-radius: 0;
    border: 1px solid $brand-primary-color;
        i {
        color: $brand-secondary-color;  
        } 
    }
    &:focus, &:active {
    color: $mid-gray;
    background-color: $white;
    border-radius: 0;
    border: 1px solid $mid-gray;
        i {
        color: $mid-gray;  
        }               
    }
    &:hover, &:active, &:focus {
    outline: none;
    box-shadow: none;
    }       
}

