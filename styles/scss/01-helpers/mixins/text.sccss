// TRANSITIONS

@mixin easing-transition () {
transition: all 0.3s ease 0s;
-webkit-transition: all 0.3s ease 0s;
-moz-transition: all 0.3s ease 0s;
}

@mixin easing-transition-block () {
transition: none;
-webkit-transition: none;
-moz-transition: none;
}


//ROTATION 

@mixin rotation ($deg) {
-webkit-transform: rotate($deg);
-moz-transform: rotate($deg);
-ms-transform: rotate($deg);
-o-transform: rotate($deg);
filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=3);
}

// INPUT

@mixin input () {
color: $brand-primary-color;
font-family: $body-font;
font-size: 14px;
font-weight: 500;
line-height: 1;    
text-align: left;   
border:1px solid $brand-primary-color;
border-radius: 0; 
}


// SELECT
@mixin select () {
height: 34px;
padding: 6px 12px;   
color: $brand-primary-color;
font-family: $body-font;
font-size: 13px;
font-weight: 500;
line-height: 1;      
border:1px solid $brand-primary-color;
border-radius: 0;
}

// SELECT OPTION
@mixin select-option () {
font-size: 11px;
color: $brand-primary-color;
font-family: $body-font;    
font-size: 14px;
line-height: 1;
font-weight: 500;
border-radius: 0;
border: 1px solid $brand-primary-color;
    &:hover {
    background: $brand-primary-color;
    color: $white;
    }
}



// PLACEHOLDER

@mixin placeholder-input () {
    ::-moz-placeholder { /* Firefox 19+ */
    color: $light-mid-gray;
    font-family: $body-font;
    font-size: 14px;
    font-weight: 500;
    line-height: 1.5;
    vertical-align: middle;          
    text-align: left;
    }
    ::-webkit-input-placeholder {
    color: $light-mid-gray;
    font-family: $body-font;
    font-size: 14px;
    font-weight: 500;
    line-height: 1;
    vertical-align: middle;       
    text-align: left;
    }
    :-moz-placeholder { /* Firefox 18- */
    color: $light-mid-gray;
    font-family: $body-font;
    font-size: 14px;
    font-weight: 500;
    line-height: 1;
    vertical-align: middle;       
    text-align: left;
    }
    :-ms-input-placeholder {
    color: $light-mid-gray;
    font-family: $body-font;
    font-size: 14px;
    font-weight: 500;
    line-height: 1;
    vertical-align: middle;  
    text-align: left;
    }    
}

