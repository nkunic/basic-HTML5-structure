@mixin full-button () {
font-size: 13px;
color: $white;
font-weight: 600;
line-height: 1;
font-family: $body-font;
background-color: $brand-primary-color;
text-transform: uppercase;
padding: 8px 25px;
border-radius: 0;
transition: all 0.3s ease 0s;
-webkit-transition: all 0.3s ease 0s;
-moz-transition: all 0.3s ease 0s;
    &:hover, &:active, &:focus {
    font-size: 13px;
    color: $white !important;
    font-weight: 600;
    line-height: 1;
    font-family: $body-font;
    background-color: $brand-secondary-color;
    text-transform: uppercase;
    padding: 8px 25px;
    border-radius: 0;   
    }
    &:active {
    font-size: 13px;
    color: $white;
    font-weight: 600;
    line-height: 1;
    font-family: $body-font;
    background-color: $mid-gray;
    text-transform: uppercase;
    padding: 8px 25px;
    border-radius: 0;          
    }    
    &:hover, &:active, &:focus {
    /*border: none;*/
    outline: none;
    box-shadow: none;
    }    
}


@mixin full-button-cancel () {
font-size: 13px;
color: $white;
font-weight: 600;
line-height: 1;
font-family: $body-font;
background-color: $mid-gray;
text-transform: uppercase;
padding: 8px 25px;
border-radius: 0;
transition: all 0.3s ease 0s;
-webkit-transition: all 0.3s ease 0s;
-moz-transition: all 0.3s ease 0s;
    &:hover, &:active, &:focus {
    font-size: 13px;
    color: $white;
    font-weight: 600;
    line-height: 1;
    font-family: $body-font;
    background-color: darken( $mid-gray, 10% );
    text-transform: uppercase;
    padding: 8px 25px;
    border-radius: 0;   
    }
    &:hover, &:active, &:focus {
    /*border: none;*/
    outline: none;
    box-shadow: none;
    }    
}




@mixin hollow-button-primary-color () {
padding: 8px 15px;	
color: $brand-primary-color;
font-family: $body-font;
font-size: 14px;
line-height: 1;
font-weight: 500;
text-transform: uppercase;
background-color: $white;
border-radius: 0;
border: 1px solid $brand-primary-color;
transition: all 0.3s ease 0s;
-webkit-transition: all 0.3s ease 0s;
-moz-transition: all 0.3s ease 0s;
	&:hover {
	color: $brand-secondary-color !important;
	background-color: $white;
	border-radius: 0;
	border: 1px solid $brand-secondary-color;			
	}
	&:focus, &:active {
	color: $white !important;
	background-color: $brand-secondary-color;
	border-radius: 0;
	border: 1px solid $brand-secondary-color;			
	}
    &:hover, &:active, &:focus {
    outline: none;
    box-shadow: none;
    }   	
}